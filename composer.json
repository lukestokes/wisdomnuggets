        "description": "php FIO client offline sign eosphp-ecc",
        "version": "0.0.1",
        "source": {
          "url": "https://github.com/lukestokes/fio-client.git",
          "type": "git",
          "reference": "master"
        },

        "authors": [
            {
                "name": "赵波",
                "email": "792598794@qq.com"
            },
            {
                "name": "Luke Stokes",
                "email": "luke.stokes@gmail.com"
            }
        ],
        "require": {
            "simplito/elliptic-php": "^1.0",
            "guzzlehttp/guzzle": "^6.3",
            "stephenhill/base58": "^1.1",
            "ext-json": "*",
            "ext-openssl": "*"
        },
        "autoload": {
            "psr-4": {
                "xtype\\": "src/"
